{
    "name": "YouTube-to-Notion",
    "description": "Take your favorite YouTube videos and add them to your Notion!",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_title": "YT-to-Notion",
        "default_popup": "popups/popup.html",
        "default_icon": "images/notionLogo.png"
    },
    "host_permissions": [
        "https://*.notion.so/*"
    ],
    "background": {
        "service_worker": "scripts/service-worker.js"
    }
}